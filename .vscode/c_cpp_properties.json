{
	"configurations": [
		{
			"name": "Mac",
			"includePath": ["/usr/local/include/c++/**"],
			"intelliSenseMode": "macos-gcc-x64",
			"compilerPath": "/usr/local/bin/g++-14",
			"cStandard": "gnu17",
			"cppStandard": "gnu++17"
		},
		{
			"name": "Win32",
			"includePath": ["${workspaceFolder}/**", "c:\\mingw64\\include\\**"],
			"intelliSenseMode": "windows-gcc-x64",
			"compilerPath": "c:\\mingw64\\bin\\g++.exe",
			"compilerArgs": ["-static-libgcc", "-static-libstdc++"],
			"cStandard": "gnu11",
			"cppStandard": "gnu++11"
		},
		{
			"name": "Linux",
			"includePath": ["${workspaceFolder}/**", "/usr/include/x86_64-linux-gnu/**"],
			"intelliSenseMode": "linux-gcc-x64",
			"compilerPath": "/usr/bin/g++",
			"cStandard": "gnu17",
			"cppStandard": "gnu++17"
		}
	],
	"version": 4
}
